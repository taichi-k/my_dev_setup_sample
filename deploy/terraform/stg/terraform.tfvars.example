# Environment Configuration
environment  = "stg"
project_name = "myproject"
aws_region   = "ap-northeast-1"

# ========================================
# App Service Variables
# ========================================

app_secret_key_for_session_middleware = "your-secret-key-here"
app_google_client_id                  = "your-google-client-id.apps.googleusercontent.com"
app_google_client_secret              = "your-google-client-secret"
app_google_auth_redirect_url          = "https://your-alb-endpoint.elb.amazonaws.com/auth/callback"
app_database_url                      = "postgresql+asyncpg://username:password@your-rds-endpoint.rds.amazonaws.com:5432/dbname"
app_database_url_sync                 = "postgresql+psycopg2://username:password@your-rds-endpoint.rds.amazonaws.com:5432/dbname"
app_otel_exporter_otlp_endpoint       = "otel-collector:4317"
app_otel_service_name                 = "app"
app_otel_service_version              = "1.0.0"
app_otel_service_namespace            = "myproj"
app_otel_deployment_environment       = "stg"
app_otel_logs_exporter                = "otlp"
app_otel_exporter_otlp_protocol       = "http/protobuf"
app_sentry_dsn                        = "https://your-sentry-dsn@sentry.io/project-id"
app_aws_region                        = "ap-northeast-1"
app_sqs_queue_name                    = "your-queue-name"
app_sqs_dlq_name                      = "your-queue-name-dlq"
app_sqs_queue_url                     = "https://sqs.ap-northeast-1.amazonaws.com/123456789012/your-queue-name"
app_sqs_dlq_url                       = "https://sqs.ap-northeast-1.amazonaws.com/123456789012/your-queue-name-dlq"
app_redis_host                        = "your-redis-endpoint.cache.amazonaws.com"
app_redis_port                        = "6379"
app_redis_use_tls                     = "true"

# ========================================
# Worker Service Variables
# ========================================

worker_database_url      = "postgresql+asyncpg://username:password@your-rds-endpoint.rds.amazonaws.com:5432/dbname"
worker_database_url_sync = "postgresql+psycopg2://username:password@your-rds-endpoint.rds.amazonaws.com:5432/dbname"
worker_aws_region        = "ap-northeast-1"
worker_sqs_queue_name    = "your-queue-name"
worker_sqs_dlq_name      = "your-queue-name-dlq"
worker_sqs_queue_url     = "https://sqs.ap-northeast-1.amazonaws.com/123456789012/your-queue-name"
worker_sqs_dlq_url       = "https://sqs.ap-northeast-1.amazonaws.com/123456789012/your-queue-name-dlq"

# ========================================
# OTEL Collector Variables
# ========================================

otel_collector_loki_host = "loki"
otel_collector_loki_port = "3100"
